{
  "data": {
    "dataset_url": "hf://datasets/MaxPrestige/credit-card-fraud-CLEAN/credit-card-fraud-CLEAN.csv",
    "root_data_dir": "./Data",
    "data_file_path": "credit-card-fraud-CLEAN.csv",
    "data_splits_dir": "DataSplits",
    "scaler_dir": "Scalers",
    "target_column": "is_fraud",
    "extra_dropped_columns": []
  }
  ,
  "parser_defaults": {
    "epochs": 8,
    "learning_rate": 0.0003,
    "max_grad_norm": 3.0,
    "dataloader_batch_size": 64,
    "dataloader_pin_memory": false,
    "dataloader_num_workers": 0,
    "log_iterations": 32,
    "eval_iterations": 32,
    "use_cuda": false,
    "device": "cpu",
    "save_model": false,
    "model_output_path": "./models/trained-model.pt"
  },
  "model": {
    "in_dim": 9,
    "intermediate_dim": 128,
    "out_dim": 2,
    "num_blocks": 12,
    "dropout_rate": 0.1
  },
  "logging": {
    "log_to_file": true,
    "log_file": "logs/app.log",
    "logger_name": "main",
    "log_level": "INFO",
    "log_mode": "w",
    "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "date_format": "%Y-%m-%d %H:%M:%S",
    "log_to_console": true
  },
  "training": {
    "optimizer": "adamw",
    "loss_function": "CrossEntropyLoss",
    "early_stopping": false,
    "early_stopping_patience": 10,
    "checkpoint_dir": "./checkpoints"
  },
  "simulated_args": {
    "--epochs": "2",
    "--learning_rate": "0.003",
    "--log_iterations": "256",
    "--eval_iterations": "64",
    "--dataloader_pin_memory": false,
    "--use_cuda": false,
    "--save_model": false,
    "--model_output_path": "../models/my-model.pt"
  }
}